{
  "defaultEnv": { "ENV": "NODE_ENV" },
  "sql-file" : true,
  "development": {
    "driver": "pg",
    "user": { "ENV" : "DB_USER" },
    "host": { "ENV" : "DB_HOST" },
    "database": { "ENV" : "DB_NAME" },
    "password": { "ENV" : "DB_PASSWORD" },
    "port": { "ENV" : "DB_PORT" },
    "ssl": { "ENV" : "DB_SSL" }
  },
  "test": {
    "driver": "pg",
    "user": { "ENV" : "DB_USER_TEST" },
    "host": { "ENV" : "DB_HOST_TEST" },
    "database": { "ENV" : "DB_NAME_TEST" },
    "password": { "ENV" : "DB_PASSWORD_TEST" },
    "port": { "ENV" : "DB_PORT_TEST" },
    "ssl": { "ENV" : "DB_SSL_TEST" }
  }
}